{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
    "getminifiedredirect": {
      "matchCondition": {
        "methods": [ "GET" ],
        "route": "/{slug}"
      },
      "backendUri": "https://%WEBSITE_HOSTNAME%/api/{slug}"
    },
    "acmechallenge": {
      "matchCondition": {
        "methods": [ "GET", "POST" ],
        "route": "/.well-known/acme-challenge/{*rest}"
      },
      "backendUri": "https://%WEBSITE_HOSTNAME%/api/letsencrypt/{rest}"
    }
  }
}